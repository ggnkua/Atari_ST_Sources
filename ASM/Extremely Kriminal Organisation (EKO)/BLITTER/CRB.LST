OPEN "o",#1,"crb.s"
CLS
INPUT "xmax";xmax
INPUT "ymax";ymax
xmax=319-16
ymax=199-16
mx=xmax/2
my=ymax/2
ok:
INPUT "coef1";c1
INPUT "coef2";c2
CLS
px=mx+mx*COSQ(0)
py=my+my*SINQ(0)
FOR i=0 TO 360 STEP 1
  x=mx+mx*COSQ(i*c1)
  y=my+my*SINQ(i*c2)
  PRINT #1,"dc.w",x AND 15
  PRINT #1,"dc.w",INT((x AND NOT (15))/2+ROUND(y)*160)
  '  ~INP(2)
  PLOT x*2,y
NEXT i
x=mx+mx*COSQ(360*c1)
y=my+my*SINQ(360*c2)
IF px=x AND py=y
  PRINT "er"
ENDIF
CLOSE #1
GOTO ok
