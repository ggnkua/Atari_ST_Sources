trackmuzak:	movem.l d0/d1/a3,-(sp)
		move.l 	frames,a3
		jsr 	noisemod+$5128		;$5124
		bsr.L 	set_frame
		move.b 	#0,$ffff8901.w
		move.b 	#1,$ffff8901.w
		bsr.L 	change_frame
		movem.l (sp)+,d0/d1/a3
		rts

