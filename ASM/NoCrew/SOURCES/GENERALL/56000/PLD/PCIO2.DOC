    ==== P22V10 Programmed Logic ====


RESET.D   = (  !AEN & !A14 & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOW 
             #   IOW & RESET.Q 
             #   A4 & RESET.Q 
             #   A5 & RESET.Q 
             #   !A6 & RESET.Q 
             #   A7 & RESET.Q 
             #   !A8 & RESET.Q 
             #   !A9 & RESET.Q 
             #   AEN & RESET.Q );
RESET.C   = (  CLK );

HRw.D   = (  !IOR & !HRw.Q 
           #   IOW & HRw.Q );
HRw.C   = (  CLK );

HEN     = (  !Q1.Q & !Q2.Q 
           #   !Q0.Q & !Q2.Q 
           #   !Q0.Q & !Q1.Q );

Ben     = (  !Q1.Q & !Q2.Q );

Q0.D   = (  !AEN & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOW & Q1.Q & !Q2.Q 
          #   !AEN & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOR & Q1.Q & !Q2.Q 
          #   !AEN & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOW & !Q0.Q & !Q2.Q 
          #   !AEN & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOR & !Q0.Q & !Q2.Q 
          #   !Q0.Q & Q1.Q & !Q2.Q );
Q0.C   = (  CLK );

Q1.D   = (  !Q0.Q & Q1.Q & !Q2.Q 
          #   !AEN & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOR & Q0.Q & !Q2.Q 
          #   !AEN & A9 & A8 & !A7 & A6 & !A5 & !A4 & !IOW & Q0.Q & !Q2.Q 
          #   Q0.Q & !Q1.Q & !Q2.Q );
Q1.C   = (  CLK );

Q2.D   = (  IOR & IOW & Q0.Q & Q1.Q & !Q2.Q 
          #   A4 & Q0.Q & Q1.Q & !Q2.Q 
          #   A5 & Q0.Q & Q1.Q & !Q2.Q 
          #   !A6 & Q0.Q & Q1.Q & !Q2.Q 
          #   A7 & Q0.Q & Q1.Q & !Q2.Q 
          #   !A8 & Q0.Q & Q1.Q & !Q2.Q 
          #   !A9 & Q0.Q & Q1.Q & !Q2.Q 
          #   AEN & Q0.Q & Q1.Q & !Q2.Q );
Q2.C   = (  CLK );

MODA     = !(  !IRQA & RESET.Q );

MODB     = (  IRQB & RESET.Q );




