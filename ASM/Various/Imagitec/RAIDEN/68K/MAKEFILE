#====================================================================
#
# Makefile	JAGOBJS.ABS
#
#====================================================================
#
#====================================================================
#	Paths
#====================================================================
#
HOME   = c:\jaguar
#
OBJECT = $(HOME)
#
SRC    = $(HOME)
#
COMMON = $(HOME)
#
INC    = $(HOME)
#
#====================================================================
#	Macro & Assembler flags
#====================================================================
#
STADDR =	3000
#
ALNFLAGS = -l -a $(STADDR) x 3e000
#
#====================================================================
#	Default Rules
#====================================================================
.SUFFIXES:	.o .s

.s.o:
	mac $*

.SUFFIXES:	.o .gas

.gas.o:
	gasm -o -CGPU -R1 -S $*
	ltxconv -lSORTER $*


#====================================================================
#	Dependencies
#====================================================================
#

#
#====================================================================
#	EXECUTABLES
#====================================================================
#


raiden.abs: raiden.o raidengs.o syn.o gpucrc.o
	aln $(ALNFLAGS) raiden.o syn.o gpucrc.o raidengs.o
	filefix3 raiden.abs

#boot.abs: boot.o 
#	aln $(ALNFLAGS) boot.o
#	filefix3 boot.abs

checksum: checksum.db

checksum.db: checksum.o
	aln $(ALNFLAGS) checksum.o
	filefix3 checksum.abs


#====================================================================
#		EOF
#====================================================================

