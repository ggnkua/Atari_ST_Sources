ax=0
ay=0
k=32
t=0
FOR y=1 TO 199
  FOR x=1 TO 320
    x0=x
    y0=y
    cx=ax
    cy=ay
    REPEAT
      u=(x0*x0-y0*y0)+cx
      y0=(2*x0*y0)+cy
      x0=u
      t=t+1
      IF t=15-1
        t=1
        n=n+1
      ENDIF
    UNTIL x0*x0+y0*y0>4
    IF n=k
      t=0
    ENDIF
    COLOR t
    PLOT x,y
  NEXT x
NEXT y
