' -------------------------------------------
' Cr‚ation de la bibliothŠque de bootsecteurs
' BibliothŠque version 4.24
'
' ð 1 mai 2001
'
' -------------------------------------------
' Format:
'
' 04 octets: 'PROT'
' 02 octets: Nombre de boots
' 10 octets: 10 valeurs pour le test du boot
' 18 octets: Nom Du boot
' 02 octets: 0
'
' 30006 octets = 6 + 30 * 1000 boots
'
$C+
INLINE biblio%,30006
INLINE decd%,20
CLS
a$="|\-/"
ptr|=1
PRINT AT(1,1);"Sauvegarde en cours ..."
@cree_bib(424)
PRINT "OK, BibliothŠque sauv‚e !"
END
'
' *********************************************
' SOUS PROGRAMME DE CREATION DE LA BIBLIOTHEQUE
' *********************************************
'
> PROCEDURE cree_bib(nombre_boot&)
  LPOKE biblio%,CVL("PROT")
  DPOKE ADD(biblio%,4),nombre_boot&
  adr%=ADD(biblio%,6)
  FOR n&=0 TO SUB(nombre_boot&,1)
    @ca_bouge
    ' -----------------
    ' 10 Octets de test
    ' -----------------
    FOR t|=0 TO 9
      READ boot$
      POKE adr%,VAL("&h"+boot$)
      INC adr%
    NEXT t|
    ' -----------
    ' Nom du boot
    ' -----------
    READ nom$
    l&=LEN(nom$)
    FOR f&=1 TO LEN(nom$)
      POKE adr%,ASC(MID$(nom$,f&,1))
      INC adr%
    NEXT f&
    ' ----------------------
    ' Remplissage avec des 0
    ' ----------------------
    IF l&<20
      FOR f&=l&+1 TO 20
        POKE adr%,0
        INC adr%
      NEXT f&
    ENDIF
  NEXT n&
  '
  '
  '
  ~C:decd%(L:biblio%+6)
  ' ----------
  ' Sauvegarde
  ' ----------
  BSAVE "PROT_BIB.DTA",biblio%,6+nombre_boot&*30
RETURN
> PROCEDURE ca_bouge
  PRINT n&
  ' PRINT AT(25,1);MID$(a$,ptr|,1)
  ' INC ptr|
  ' IF ptr|>4
  ' ptr|=1
  ' ENDIF
RETURN
