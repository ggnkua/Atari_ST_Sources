' *** INS_SRT.LST ***                                  2nd GFA-Manual, page 5-7
> PROCEDURE insert_sort(VAR proc())
  '
  ' *** Sort word-array with 'Insertion Sort'
  '
  LOCAL last,i,x,j
  last=PRED(DIM?(proc()))
  FOR i=1 TO last
    IF proc(i)<proc(0)
      SWAP proc(i),proc(0)
    ENDIF
  NEXT i
  FOR i=2 TO last
    x=proc(i)
    j=i
    WHILE x<proc(PRED(j))
      proc(j)=proc(PRED(j))
      DEC j
    WEND
    proc(j)=x
  NEXT i
RETURN
' *********
