  ENDIF
  IF e$="A"
    GOSUB lagerkennzahlen
  ENDIF
  IF e$="Z"
    GOSUB zeiteinheiten
  ENDIF
  IF e$="P"
    GOSUB prozentrechnung
  ENDIF
