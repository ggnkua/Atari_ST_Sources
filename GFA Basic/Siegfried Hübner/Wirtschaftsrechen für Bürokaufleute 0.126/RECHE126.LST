PROCEDURE primz_11
  GOSUB titel
  PRINT AT(3,4);STRING$(16,45);" P r i m z a h l e n  von 1 bis ";er;" ";STRING$(16,45)
  PRINT
  IF er<1 OR er>1000
    ALERT 3,"Ich kann nur die|Primzahlen von 1-1000|berechnen",1,"Sorry",d%
    GOTO lemik
  ENDIF
  FOR x=2 TO er
    y=2
    WHILE y<x/2
      EXIT IF x MOD y=0
      INC y
    WEND
    IF x MOD y<>0 OR x=2
      PRINT USING "####",x;
    ENDIF
  NEXT x
  lemik:
  GOSUB com
RETURN
