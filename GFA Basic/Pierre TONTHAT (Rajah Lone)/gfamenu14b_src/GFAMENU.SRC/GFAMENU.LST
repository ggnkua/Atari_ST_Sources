> PROCEDURE compiler_1 ! options de compil
  co$=""
  IF co!(6) ! interruptions
    co$=co$+" I+"
  ENDIF
  IF co!(7) ! Select
    co$=co$+" S<"
  ENDIF
  IF co!(8) ! Fonctions
    co$=co$+" F>"
  ELSE
    co$=co$+" F<"
  ENDIF
  IF co!(9) ! ProcÇdures
    co$=co$+" P>"
  ELSE
    co$=co$+" P<"
  ENDIF
  IF co!(10) ! IntDiv
    co$=co$+" %3"
  ELSE
    co$=co$+" %0"
  ENDIF
  IF co!(11) ! IntMul
    co$=co$+" *&"
  ELSE
    co$=co$+" *%"
  ENDIF
  IF co!(12) ! Erreurs
    co$=co$+" E$"
  ELSE
    co$=co$+" E-"
  ENDIF
  IF co!(17) ! Debug Symbol
    s$="-s"
  ELSE
    s$=""
  ENDIF
  m$=CHAR{{OB_SPEC(adtree%(3),15)}}
  memory%=VAL(m$)
  IF memory%>0 AND memory%<100000000
    co$=co$+" $m"+STR$(memory%)
  ENDIF
RETURN
