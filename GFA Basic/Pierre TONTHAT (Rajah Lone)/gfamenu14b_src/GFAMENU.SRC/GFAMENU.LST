> PROCEDURE noir_blanc(arbre%,fils%,nb_etat%)
  IF BTST(OB_FLAGS(adtree%(arbre%),fils%),9)=0
    black_white(arbre%,fils%,OB_STATE(adtree%(arbre%),fils%))
  ENDIF
  black_white(arbre%,fils%,nb_etat%)
