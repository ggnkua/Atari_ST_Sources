> PROCEDURE init_1
  ap_id&=APPL_INIT()
  vdi_handle&=@v_opnvwk
  '
  ~GRAF_MOUSE(0,0)
  '
  ~WIND_UPDATE(3)
  ~WIND_UPDATE(1)
  '
  ~WIND_GET(0,4,screenx&,screeny&,screenl&,screenh&)
  '
  IF VAL(HEX$(INT{{ADD(GB,4)}}))>=399
    naes!=TRUE
  ELSE
    naes!=FALSE
  ENDIF
  '
  magic!=@test_cookie("MagX",dummy%)
  mint!=@test_cookie("MiNT",dummy%)
  IF magic! OR mint!
    multi!=TRUE
  ELSE
    multi!=FALSE
  ENDIF
  IF ap_id&=0
    multi!=FALSE
  ENDIF
  '
  @declare_1
  @declare_2
  @declare_3
  @declare_4
  IF mint!=FALSE
    RESERVE 15360
  ENDIF
  @declare_5
  '
  ~RSRC_FREE()
  IF @s_exist(chemin$+gfamenu_rsc$)=TRUE
    result%=RSRC_LOAD(chemin$+gfamenu_rsc$)
    IF result%=0
      ~FORM_ALERT(1,"[1][ GFAMENU.RSC n'a pas pu| àtre chargÇ comme fichier| ressource.][ Ok ]")
      sortir
    ELSE
      FOR i%=0 TO nb_tree%
        ~RSRC_GADDR(0,i%,adtree%(i%))
      NEXT i%
      FOR i%=1 TO 4
        ~FORM_CENTER(adtree%(i%),xd&(i%),yd&(i%),ld&(i%),hd&(i%))
      NEXT i%
    ENDIF
  ELSE
    ~FORM_ALERT(1,"[1][ GFAMENU.RSC non trouvÇ.][ Ok ]")
    sortir
  ENDIF
  '
RETURN
> PROCEDURE declare_1
  lect%=GEMDOS(25)
  chemin$=CHR$(lect%+65)+":"+DIR$(lect%+1)+"\"
  c0$=CHR$(0)
  masque$="*.*"+c0$
  gfamenu_rsc$="GFAMENU.RSC"+c0$
  gfamenu_inf$="GFAMENU.INF"+c0$
  '
  nb_tree%=5
  '
  DIM adtree%(nb_tree%),xd&(nb_tree%),yd&(nb_tree%),ld&(nb_tree%),hd&(nb_tree%)
  DIM hand_win%(nb_tree%),wx&(nb_tree%),wy&(nb_tree%),wl&(nb_tree%),wh&(nb_tree%)
  '
RETURN
> PROCEDURE declare_2
  '
  DIM win!(nb_tree%),aff!(nb_tree%)
  FOR i%=0 TO 3
    win!(i%)=FALSE
    aff!(i%)=FALSE
  NEXT i%
  '
  DIM cp_win%(nb_tree%)
  cp_win%(1)=&X1011
  cp_win%(2)=&X1011
  cp_win%(3)=&X1011
  '
RETURN
> PROCEDURE declare_3
  DIM info_chemin$(7),nom_chemin$(7),conv&(7),co!(17)
  '
  co!(6)=FALSE     ! DÇsactiver les routines d'interruptions
  co!(7)=TRUE      ! Optimiser SELECT...CASE
  co!(8)=TRUE      ! Ne pas gÇnÇrer ENDFUNC
  co!(9)=FALSE     ! pas P>, compiler en sous-routine 68000
  co!(10)=FALSE    ! pas de %3, convertir les divisions entiäres en virg. flottante
  co!(11)=FALSE    ! pas *&, MUL (MC68000) n'est pas utilisÇ
  co!(12)=TRUE     ! E$, pas de messages d'erreurs sous forme de texte
  co!(17)=FALSE    ! Aucun symbìle de Debogage
  '
  FOR i%=1 TO 6
    info_chemin$(i%)=SPACE$(256)
  NEXT i%
  '
  nom_chemin$(1)="GFABASIC.PRG"+c0$
  nom_chemin$(2)="RSC2.PRG"+c0$
  nom_chemin$(3)="GFA_BCOM.PRG"+c0$
  nom_chemin$(4)="GL.PRG"+c0$
  nom_chemin$(5)=c0$
  nom_chemin$(6)="GFA3BLIB"+c0$
  '
  FOR i%=1 TO 6
    conv&(i%)=11+i%
  NEXT i%
  '
RETURN
