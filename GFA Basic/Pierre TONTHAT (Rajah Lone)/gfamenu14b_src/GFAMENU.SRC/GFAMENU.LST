> PROCEDURE boucle_generale
  DO
    evnt&=@ev_multi(&X10011,2,1,1,100,mo_x&,mo_y&,mo_k&,m_touche%,m_clavier%,mo_c&)
    IF BTST(evnt&,0)
      m_clavier&=BYTE(m_clavier%)
      IF m_clavier&=17
        IF FORM_ALERT(1,CHAR{OB_SPEC(adtree%(nb_tree%),4)})=1
          sortir
        ENDIF
      ENDIF
      SELECT m_clavier%
      CASE 25088 ! Help
      CASE 15104 ! f1
        win(1)
      CASE 15360 ! f2
        win(2)
      CASE 15616 ! f3
        call(1)
      CASE 15872 ! f4
        call(2)
      CASE 16128 ! f5
        compiler_prg
      CASE 16384 ! f6
        tester
      CASE 16896 ! f8
      CASE 17152 ! f9
      CASE 17408 ! f10
      ENDSELECT
      ~WIND_GET(0,10,top_win%,dummy%,dummy%,dummy%)
      IF top_win%=hand_win%(3) AND win!(3)
        SELECT m_clavier&
        CASE 8,27,48 TO 57
          ~OBJC_EDIT(adtree%(3),15,m_clavier%,edit_cur%,2,edit_cur%)
        ENDSELECT
      ENDIF
    ENDIF
    IF BTST(evnt&,1)
      IF mo_c&=1 AND mo_k&=1
        ~WIND_GET(0,10,top_win%,dummy%,dummy%,dummy%)
        clic_win%=WIND_FIND(mo_x&,mo_y&)
        delai
        boucle_generale_suite
      ENDIF
    ENDIF
    IF BTST(evnt&,4)
      SELECT m_type&
      CASE 10
        boucle_menu
      CASE 20
        redraw
      CASE 21
        win_topped
      CASE 22
        win_closed
      CASE 28
        win_moved
      CASE 29,31
        win_top
      CASE 50
        QUIT
      CASE 22360
        FOR i%=1 TO 3
          IF m_fenetre&=hand_win%(i%) AND win!(i%)=TRUE
            aff!(i%)=FALSE
          ENDIF
        NEXT i%
      CASE 22361
        FOR i%=1 TO 3
          IF m_fenetre&=hand_win%(i%) AND win!(i%)=TRUE
            aff!(i%)=TRUE
          ENDIF
        NEXT i%
      ENDSELECT
    ENDIF
    clavier_win%=0
    m_entree&=0
    m_titre&=0
    m_type&=0
    m_fenetre&=0
    m_x&=0
    m_y&=0
    m_l&=0
    m_h&=0
  LOOP
RETURN
> PROCEDURE boucle_generale_suite
  IF clic_win%=hand_win%(2) AND win!(2)
    gere_chemins
  ENDIF
  IF clic_win%=hand_win%(3) AND win!(3)
    gere_dialog
  ENDIF
RETURN
> PROCEDURE boucle_menu
  ~MENU_TNORMAL(adtree%(0),m_titre&,1)
  SELECT m_entree&
  CASE 7 ! about
    win(1)
  CASE 16 ! chemins
    win(2)
  CASE 18 ! editeur GFA
    call(1)
  CASE 19 ! editeur RSC
    call(2)
  CASE 21 ! quitter
    sortir
  ENDSELECT
RETURN
