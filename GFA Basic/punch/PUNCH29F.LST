' gfa punch 29
' ligne qui tourne
b%=2
x%=100
FOR i%=0 TO 3199
  LINE COS(n%/180*PI)*50+x%,SIN(n%/180*PI)*50+y%,2*x%-(COS(n%/180*PI)*50+x%),2*y%-(SIN(n%/PI*180)*50+y%)
  ADD x%,a%
  ADD y%,b%
  ADD n%,5
  IF n%=640 OR x%=0
    a%=-a%
  ENDIF
  IF y%=400 OR y%=0
    b%=-b%
  ENDIF
NEXT i%
VOID INP(2)
