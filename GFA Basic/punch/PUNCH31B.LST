' gfa punch 31
' 512 couleurs
FOR r&=0 TO 7
  FOR b&=0 TO 7
    FOR v&=0 TO 7
      DPOKE HIMEM+208+(r&*64+b&*8+v&)*2,r&*256+v&*16+b&
    NEXT v&
  NEXT b&
NEXT r&
FOR x&=0 TO 280 STEP 40
  DEFFILL ASC(MID$(CHR$(2)+CHR$(3)+CHR$(6)+CHR$(4)+CHR$(7)+CHR$(5)+CHR$(8)+CHR$(9),x&/40+1,1))
  PBOX x&,0,x&+39,199
NEXT x&
BOX 0,0,319,199
p%=GEMDOS(32,L:0)
REPEAT
  FOR t%=HIMEM TO HIMEM+1216 STEP 16
    BMOVE t%,&HFFFF8242,16
  NEXT t%
  VSYNC
UNTIL INP(-2)
VOID GEMDOS(32,L:p%)
