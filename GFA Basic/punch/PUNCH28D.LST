' gfa punch 28
' poisson
DEFFLT "n"
DEFTEXT 1,1,1,1
phrase$="Fish ...."
FOR n=0 TO PI*4 STEP 0.015
  FOR x%=1 TO LEN(phrase$)
    ATEXT SIN(n+0.3*x%)*65+320,COS(n+0.3*x%)*65+200,2,MID$(phrase$,x%,1)
  NEXT x%
NEXT n
CLR n
phrase$="Fish food ........"
DO
  INC n
  FOR x%=1 TO LEN(phrase$)
    ATEXT x%*8+274,190+SIN((n+x%*4)/10)*20,2,MID$(phrase$,x%,1)
  NEXT x%
LOOP
