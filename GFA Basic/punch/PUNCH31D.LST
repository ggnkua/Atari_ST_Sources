' GFA PUNCH 31
' degrade
HIDEM
pas=2
col1=50
col2=50
FOR i=1 TO 8
  VSETCOLOR i,col1,col2,i-1
  VSETCOLOR i+8,col1,col2,7-i
NEXT i
DIM x(4),y(4)
FOR i=0 TO 4
  x(i)=RANDOM(320)
  y(i)=RANDOM(200)
NEXT i
FOR x=0 TO 319 STEP pas
  FOR y=0 TO 200 STEP pas
    d=1
    FOR i=0 TO 4
      d=d*((-x(i))^2+(y-y(i))^2)
    NEXT i
    DEFFILL 1+(d^0.07 AND 15)
    PBOX x,y,x+pas-1,y+pas-1
  NEXT y
NEXT x
SHOWM
EDIT
