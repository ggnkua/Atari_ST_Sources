' gfa punch 29
' affiche un fond d ecran
DIM g$(10)
FOR lo%=-199 TO 839 STEP 2
  LINE lo%-199,399,lo%+194,0
NEXT lo%
FOR lo%=0 TO 10
  GET 0,lo%*36+2,639,(lo%+1)*36+2,g$(lo%)
NEXT lo%
DO
  lo%=(lo%+1) MOD 11
  FOR lo2%=0 TO 10
    PUT 0,lo2%*36+2,g$((lo%+lo2%) MOD 11)
  NEXT lo2%
LOOP
