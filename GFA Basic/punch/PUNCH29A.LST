' gfa punch 29
' dessine un ressort
DIM a$(15)
FOR i=0 TO 15
  CLR a,r
  b=b+0.01
  REPEAT
    FOR t=0 TO 36000 STEP 25
      a=a+b
      INC r
      ELLIPSE a,100,10,15,t,t+57
    NEXT t
  UNTIL r>2000
  GET 0,84,316,116,a$(i)
  CLS
NEXT i
DO
  FOR t=0 TO 15
    PUT 0,84,a$(t)
    PAUSE 4
  NEXT t
LOOP
