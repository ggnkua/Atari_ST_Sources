' gfa punch 29
' trace des cercles
DIM image$(20)
DEFFILL 1,0
FOR n=0 TO PI/2 STEP PI/39
  l=n/0.5
  FOR m=0 TO PI STEP PI
    FOR o=0 TO 4 STEP PI/15
      q=(SIN(o)*SIN(m+l))*50
      r=(COS(o)+SIN(o)*COS(m+l))*20
      PELLIPSE 128+q,80+r,r-q,r+q
    NEXT o
  NEXT m
  INC p%
  GET 25,5,235,155,image$(p%)
  CLS
NEXT n
PRINT "voila ...."
REPEAT
  FOR p%=1 TO 20
    PUT MOUSEX,MOUSEY,image$(p%)
  NEXT p%
UNTIL INKEY$<>""
