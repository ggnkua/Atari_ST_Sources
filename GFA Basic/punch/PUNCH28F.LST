' gfa punch 28
' ERREUR
FOR x=-80 TO 80 STEP 6
  FOR y=-100 TO 100
    GOSUB calcul
    ny=y-x*0.5+150
    nz=z+x*0.5+60
    COLOR 1
    LINE ny,nz,ny,200
    IF y<-100 OR y>-100 THEN
      COLOR 0
      LINE py,pz,ny,nz
    ENDIF
    py=ny
    pz=nz
  NEXT y
NEXT x
'
PROCEDURE calcul
  xt=x*0.00500001
  yt=y*0.005000001
  z=100-6/sqrt(xt*xt+yt*yt)
RETURN
