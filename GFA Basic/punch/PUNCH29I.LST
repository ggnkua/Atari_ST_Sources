' gfa punch 29
' texte dans carre qui defile
LINE 0,0,640,0
LINE 0,199,640,199
DO
  a=SIN(x)*50+60
  COLOR 2
  BOX y,a,y+80,a+80
  DEFTEXT 1,1,0,6
  TEXT y+10,a+16,"F"
  TEXT y+18,a+27,"r"
  TEXT y+26,a+38,"E"
  TEXT y+34,a+49,"d"
  TEXT y+42,a+60,CHR$(199)
  x=x+0.02
  y=y+1
  COLOR 0
  BOX y-10,SIN(x-0.2)*50+60,y-10+80,(SIN(x-0.2)*50+60)+80
  IF y>650
    y=40
  ENDIF
LOOP
