BEGIN (* tlxmath *)
#if LONGREAL_var
 init;
#endif

 procname := "sinh";
 procR2R  := sinh;
 TestR2R(-eighty, -sinh1);
 TestR2R(-ten, -sinh2);
 TestR2R(-null5, -sinh3);
 TestR2R(null, null);
 TestR2R(one5, sinh5);
 TestR2R(ten, sinh6);
 TestR2R(eighty, sinh7);
 WriteLn;

 procname := "cosh";
 procR2R  := cosh;
 TestR2R(-eighty, cosh1);
 TestR2R(-ten, cosh2);
 TestR2R(-null5, cosh3);
 TestR2R(null, one);
 TestR2R(one5, cosh5);
 TestR2R(ten, cosh6);
 TestR2R(eighty, cosh7);
 WriteLn;

 procname := "tanh";
 procR2R  := tanh;
 TestR2R(-eighty, -one);
 TestR2R(-ten, -tanh2);
 TestR2R(-null5, -tanh3);
 TestR2R(null, null);
 TestR2R(one5, tanh5);
 TestR2R(ten, tanh6);
 TestR2R(eighty, one);
 WriteLn;

 procname := "arcsinh";
 procR2R  := arcsinh;
 TestR2R(-eighty, -arsinh1);
 TestR2R(-ten, -arsinh2);
 TestR2R(-null5, -arsinh3);
 TestR2R(null, null);
 TestR2R(one5, arsinh5);
 TestR2R(ten, arsinh6);
 TestR2R(eighty, arsinh7);
 WriteLn;

 procname := "arccosh";
 procR2R  := arccosh;
 TestR2R(one, null);
 TestR2R(two, arcosh2);
 TestR2R(ten, arcosh3);
 TestR2R(arcoshi4, arcosh4);
 TestR2R(arcoshi5, arcosh5);
 TestR2R(arcoshi6, arcosh6);
 TestR2R(arcoshi7, arcosh7);
 WriteLn;

 procname := "arctanh";
 procR2R  := arctanh;
(* TestR2R(-artanhi1, -artanh1);*)
 TestR2R(-null5, -artanh2);
 TestR2R(-null25, -artanh3);
 TestR2R(null, null);
 TestR2R(null25, artanh5);
 TestR2R(null5, artanh6);
(* TestR2R(artanhi7, artanh7);*)
 WriteLn;

 procname := "cbrt";
 procR2R  := cbrt;
 TestR2R(two, cbrt1);
 TestR2R(four, cbrt2);
 TestR2R(eight, cbrt3);
 TestR2R(ten, cbrt4);
 TestR2R(hundred, cbrt5);
 WriteLn;
END tlxmath.

