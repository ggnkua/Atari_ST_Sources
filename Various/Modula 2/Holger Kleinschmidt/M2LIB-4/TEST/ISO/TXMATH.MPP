BEGIN (* txmath *)
 procname := "sinh";
 procR2R  := sinh;
#ifndef MM2
 TestR2R(-80.0, sinh1);
#endif
 TestR2R(-10.0, sinh2);
 TestR2R(-0.5, sinh3);
 TestR2R(0.0, sinh4);
 TestR2R(1.5, sinh5);
 TestR2R(10.0, sinh6);
#ifndef MM2
 TestR2R(80.0, sinh7);
#endif
 WriteLn;

 procname := "cosh";
 procR2R  := cosh;
#ifndef MM2
 TestR2R(-80.0, cosh1);
#endif
 TestR2R(-10.0, cosh2);
 TestR2R(-0.5, cosh3);
 TestR2R(0.0, cosh4);
 TestR2R(1.5, cosh5);
 TestR2R(10.0, cosh6);
#ifndef MM2
 TestR2R(80.0, cosh7);
#endif
 WriteLn;

 procname := "tanh";
 procR2R  := tanh;
 TestR2R(-80.0, tanh1);
 TestR2R(-10.0, tanh2);
 TestR2R(-0.5, tanh3);
 TestR2R(0.0, tanh4);
 TestR2R(1.5, tanh5);
 TestR2R(10.0, tanh6);
 TestR2R(80.0, tanh7);
 WriteLn;

 procname := "arcsinh";
 procR2R  := arcsinh;
 TestR2R(-80.0, arsinh1);
 TestR2R(-10.0, arsinh2);
 TestR2R(-0.5, arsinh3);
 TestR2R(0.0, arsinh4);
 TestR2R(1.5, arsinh5);
 TestR2R(10.0, arsinh6);
 TestR2R(80.0, arsinh7);
 WriteLn;

 procname := "arccosh";
 procR2R  := arccosh;
 TestR2R(1.0, arcosh1);
 TestR2R(2.0, arcosh2);
 TestR2R(10.0, arcosh3);
 TestR2R(2.0E2, arcosh4);
 TestR2R(1.0E3, arcosh5);
 TestR2R(1.0E5, arcosh6);
 TestR2R(1.0E18, arcosh7);
 WriteLn;

 procname := "arctanh";
 procR2R  := arctanh;
(* TestR2R(-0.99999, artanh1);*)
 TestR2R(-0.5, artanh2);
 TestR2R(-0.25, artanh3);
 TestR2R(0.0, artanh4);
 TestR2R(0.25, artanh5);
 TestR2R(0.5, artanh6);
(* TestR2R(0.99999, artanh7);*)
 WriteLn;

 procname := "cbrt";
 procR2R  := cbrt;
 TestR2R(2.0, cbrt1);
 TestR2R(4.0, cbrt2);
 TestR2R(8.0, cbrt3);
 TestR2R(10.0, cbrt4);
 TestR2R(100.0, cbrt5);

END txmath.

