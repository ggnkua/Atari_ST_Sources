IMPLEMENTATION  MODULE DRAWINNDC;
IMPORT VDIControls;
IMPORT GEMVDIbase;
IMPORT AESEvents;
IMPORT VDIAttribs;
IMPORT VDIOutputs;
IMPORT GEMAESbase;

PROCEDURE DRAWNDC(HANDLE:INTEGER);
VAR XYARRAY:ARRAY [0..1] OF INTEGER;
    I: INTEGER;
BEGIN
        XYARRAY[0]:=32767 DIV 2;
        XYARRAY[1]:=32767 DIV 2;
        I:=  VDIAttribs.SetMarkerColour(HANDLE,1 (* BLACK*));
        I:=  VDIAttribs.SetMarkerType(HANDLE,1);
        VDIOutputs.PolyMarker(HANDLE,1,XYARRAY);
END DRAWNDC;

BEGIN
END DRAWINNDC.

 