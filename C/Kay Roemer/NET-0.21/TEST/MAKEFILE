#
#	Makefile for the Mint-Net "test suite"
#

all: server client sockpair dgram dgramd speed speedd sockname

CC = gcc
CFLAGS = -O -mshort
CPPFLAGS = -I../lib
LIBS = -L../lib -lsocket16

server: server.o
	$(CC) $(CFLAGS) server.o $(LIBS) -o server
	chmod +x server

client: client.o
	$(CC) $(CFLAGS) client.o $(LIBS) -o client
	chmod +x client

speed: speed.o
	$(CC) $(CFLAGS) speed.o $(LIBS) -o speed
	chmod +x speed

speedd: speedd.o
	$(CC) $(CFLAGS) speedd.o $(LIBS) -o speedd
	chmod +x speedd

sockpair: sockpair.o
	$(CC) $(CFLAGS) sockpair.o $(LIBS) -o sockpair
	chmod +x sockpair

dgram: dgram.o
	$(CC) $(CFLAGS) dgram.o $(LIBS) -o dgram
	chmod +x dgram

dgramd: dgramd.o
	$(CC) $(CFLAGS) dgramd.o $(LIBS) -o dgramd
	chmod +x dgramd

sockname: sockname.o
	$(CC) $(CFLAGS) sockname.o $(LIBS) -o sockname
	chmod +x sockname

# No dependencies jet
depend:

clean:
	rm -f *.o
