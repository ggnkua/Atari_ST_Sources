# Makefile for mined

CC=acc --shared
CFLAGS	= -I../../include -O2 -D_MINIX -D_POSIX_SOURCE

OBJ = mined1.o mined2.o

all:	mined

mined:	$(OBJ)
	$(CC) -o $@ $(OBJ)
	install -S 64k $@

install:	/usr/bin/mined

/usr/bin/mined:	mined
	install -cs -o bin -g bin -m 555 mined $@

$(OBJ):	mined.h

clean:
	rm -f *.o *.s core *.bak

clobber:
	rm -f mined *.o *.s core *.bak

