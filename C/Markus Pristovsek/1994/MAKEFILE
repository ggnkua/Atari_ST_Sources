## Makefile fuer die Drucker-Treiber von DVI## 6.5.93 Marcus Haebler, Basis# 8.10.93 Markus Pristovsek, diverse Ergaenzungen, ungetestet!# 01.09.94 Markus Kilbinger, einige Bugs beseitigt, Treiber getestet.## Wann kommt der naechste Markus?## Achtung: Alle Dateien mit der Endung .GAS muessen in .S fuer GNU-C #          umbenannt werden, DREHE_90.S GELOeSCHT!!!#CC = gccAS = gcc# Optionen fuer TT & Falcon
#CFLAGS = -Wall -mshort -O2 -fomit-frame-pointer -m68030 -m68881 -I.CFLAGS = -Wall -mshort -O2 -fomit-frame-pointer -I.ASFLAGS = -cOBJS = fast_dvi.o treiber.oall: deskjet.ttp epson9.ttp epson_90.ttp img.ttp img_90.ttp jp350ws.ttp \     jp_90.ttp lj600.ttp lj600_90.ttp ljII.ttp ljII_90.ttp necp7.ttp stylus.ttp \     stylus90.ttp vdi.ttp vdi_90.ttp lq800.ttpdeskjet.ttp: deskjet.o $(OBJS)		 $(CC) $(CFLAGS) -o deskjet.ttp $(OBJS) deskjet.o		 strip deskjet.ttp		 size deskjet.ttpepson9.ttp: epson9.o $(OBJS)		 $(CC) $(CFLAGS) -o epson9.ttp $(OBJS) epson9.o block_by.o		 strip epson9.ttp		 size epson9.ttpepson_90.ttp: epson_90.o $(OBJS) drehe_90.o		 $(CC) $(CFLAGS) -o epson_90.ttp $(OBJS) epson_90.o block_by.o drehe_90.o		 strip epson_90.ttp		 size epson_90.ttpimg.ttp: img.o treiber.o		 $(CC) $(CFLAGS) -o img.ttp $(OBJS) img.o		 strip img.ttp		 size img.ttpimg_90.ttp: img_90.o treiber.o drehe_90.o		 $(CC) $(CFLAGS) -o img_90.ttp $(OBJS) img_90.o		 strip img_90.ttp		 size img_90.ttpjp350ws.ttp: jp350ws.o $(OBJS)		 $(CC) $(CFLAGS) -o jp350ws.ttp $(OBJS) jp350ws.o		 strip jp350ws.ttp		 size jp350ws.ttpjp_90.ttp: jp_90.o $(OBJS) drehe_90.o		 $(CC) $(CFLAGS) -o jp_90.ttp $(OBJS) jp_90.o drehe_90.o		 strip jp_90.ttp		 size jp_90.ttplj600.ttp: laser600.o $(OBJS)		 $(CC) $(CFLAGS) -o laser600.ttp $(OBJS) laser600.o		 strip lj600.ttp		 size lj600.ttplj600_90.ttp: lj600_90.o $(OBJS) drehe_90.o		 $(CC) $(CFLAGS) -o lj600_90.ttp $(OBJS) lj600_90.o drehe_90.o		 strip lj600_90.ttp		 size lj600_90.ttp
ljII.ttp: ljII.o $(OBJS)		 $(CC) $(CFLAGS) -o ljII.ttp $(OBJS) ljII.o		 strip ljII.ttp		 size ljII.ttpljII_90.ttp: ljII_90.o $(OBJS) drehe_90.o		 $(CC) $(CFLAGS) -o ljII_90.ttp $(OBJS) ljII_90.o drehe_90.o		 strip ljII_90.ttp		 size ljII_90.ttpnecp7.ttp: necp7.o $(OBJS)		 $(CC) $(CFLAGS) -o necp7.ttp $(OBJS) necp7.o block_by.o		 strip necp7.ttp		 size necp7.ttplq800.ttp: lq800.o $(OBJS)		 $(CC) $(CFLAGS) -o lq800.ttp $(OBJS) lq800.o block_by.o		 strip lq800.ttp		 size lq800.ttpstylus.ttp: stylus.o $(OBJS)		 $(CC) $(CFLAGS) -o stylus.ttp $(OBJS) stylus.o block_by.o		 strip stylus.ttp		 size stylus.ttpstylus90.ttp: stylus90.o $(OBJS)		 $(CC) $(CFLAGS) -o stylus90.ttp $(OBJS) stylus90.o block_by.o		 strip stylus.ttp		 size stylus.ttpvdi.ttp: vdi.o treiber.o		 $(CC) $(CFLAGS) -o vdi.ttp $(OBJS) vdi.o		 strip vdi.ttp		 size vdi.ttpvdi_90.ttp: vdi_90.o treiber.o drehe_90.o		 $(CC) $(CFLAGS) -o vdi_90.ttp $(OBJS) vdi_90.o		 strip vdi_90.ttp		 size vdi_90.ttptreiber.o: treiber.hdeskjet.o: hp.cjp350ws.o: hp.cjp_90.o: hp_90.claser600.o: hp.clj600_90.o: hp_90.cljII.o: hp.cljII_90.o: hp_90.cnecp7.o: 24nadel.c
lq.o: 24nadel.c