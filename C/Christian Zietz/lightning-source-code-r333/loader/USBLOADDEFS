ifeq ($(usbload),000)
TARGET = ./loader.prg
CPU = 000
endif

ifeq ($(usbload),02060)
TARGET = ./loader.prg
CPU = 020-60
endif

ifeq ($(usbload),030)
TARGET = ./loader.prg
CPU = 030
endif

ifeq ($(usbload),040)
TARGET = ./loader.prg
CPU = 040
endif

ifeq ($(usbload),060)
TARGET = ./loader.prg
CPU = 060
endif

ifeq ($(usbload),col)
TARGET = ./loader.prg
CPU = v4e
endif

usbloadtargets = 000 02060 030 040 060 col
