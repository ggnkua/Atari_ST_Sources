DEFINES = -DUNIX
COMPILERSWITCHES = -DNDEBUG
LINKERSWITCHES = -s
CC = /usr/bin/cc

all:	linkinfo
linkinfo:	linkinfo.o mygetopt.o
	$(CC) -olinkinfo $(LINKERSWITCHES) linkinfo.o mygetopt.o
linkinfo.o:	linkinfo.c
	$(CC) -c $(COMPILERSWITCHES) $(DEFINES) linkinfo.c
mygetopt.o:	mygetopt.c
	$(CC) -c $(COMPILERSWITCHES) $(DEFINES) mygetopt.c
