' getxbra() in OMIKRON-Basic
' Vec%l, Name$ und Nxt%l sind global, da
' RÅckgabeparameter in Fkt. nicht erlaubt

DEF FN Getxbra%
  LOCAL L%L,I%
  L%L= LPEEK(Vec%L)
  IF ( LPEEK(L%L-12)=$58425241) THEN
    Name$=""
    FOR I%=-8 TO -5
      Name$=Name$+ CHR$( PEEK(L%L+I%))
    NEXT I%
    Nxt%L=L%L-4
    RETURN 1
  ENDIF
RETURN 0

