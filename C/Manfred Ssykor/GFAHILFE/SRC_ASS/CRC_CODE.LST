INLINE ad%,107
crc%=@crc(ad%,107)
PRINT HEX$(crc%),crc%
> FUNCTION crc(adr%,anz%)
LOCAL crc_code%
' CRC-Pruefsummenermittlung nach dem CCITT-Polynom x^16+x^12+x^5+1
INLINE crc_code%,40
' Es wird die 16-Bit CRC-Summe ab adr% ueber anz% Byte gebildet.
RETURN C:crc_code%(L:adr%,anz%)
ENDFUNC
